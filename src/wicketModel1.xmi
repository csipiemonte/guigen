<?xml version="1.0" encoding="ASCII"?>
<guigen:GUIModel xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:guigen="http://www.csi.it/mddtools" xsi:schemaLocation="http://www.csi.it/mddtools metamodel/guigen.ecore" codProdotto="prod1" codComponente="comp1" versioneProdotto="0.0.1" versioneComponente="0.0.1">
  <structure>
    <appWindow>
      <footer/>
      <header/>
      <appArea>
        <menubar>
          <topLevelMenu name="form" label="Esempi di Form">
            <item name="simpleForm" label="Form Semplice">
              <eventHandler>
                <action xsi:type="guigen:JumpAction" jumpTo="//@structure/@appWindow/@appArea/@contentPanels.1"/>
              </eventHandler>
            </item>
          </topLevelMenu>
          <topLevelMenu name="top2" label="Top Menu 2">
            <item name="buttonsItm" label="Esempio Buttons">
              <eventHandler>
                <action xsi:type="guigen:JumpAction" jumpTo="//@structure/@appWindow/@appArea/@contentPanels.2"/>
              </eventHandler>
            </item>
          </topLevelMenu>
          <topLevelMenu name="top3" label="Top Menu 3">
            <item name="item31" label="Item 3.1">
              <eventHandler>
                <action xsi:type="guigen:JumpAction" jumpTo="//@structure/@appWindow/@appArea/@contentPanels.0"/>
              </eventHandler>
            </item>
            <item name="item32" label="Item 3.2">
              <eventHandler>
                <action xsi:type="guigen:JumpAction" jumpTo="//@structure/@appWindow/@appArea/@contentPanels.1"/>
              </eventHandler>
            </item>
          </topLevelMenu>
        </menubar>
        <contentPanels name="start"/>
        <contentPanels name="esempioForm">
          <panels xsi:type="guigen:FormPanel" name="contatto" label="Contatto">
            <widgets xsi:type="guigen:TextField" name="cognome" label="Cognome:" fieldLength="50"/>
            <widgets xsi:type="guigen:TextField" name="nome" label="Nome:" fieldLength="50"/>
            <widgets xsi:type="guigen:TextArea" name="note" label="Osservazioni:" rows="5" columns="20"/>
            <widgets xsi:type="guigen:CheckBox" name="abilitato" label="Abilitato:"/>
            <widgets xsi:type="guigen:ComboBox" name="regione" label="Regione:"/>
            <widgets xsi:type="guigen:ComboBox" name="provincia" label="Provincia:" dataTypeModifier=""/>
            <widgets xsi:type="guigen:ConfirmButton" name="submit" label="Ok">
              <eventHandlers>
                <action xsi:type="guigen:JumpAction" jumpTo="//@structure/@appWindow/@appArea/@contentPanels.0"/>
              </eventHandlers>
            </widgets>
            <widgets xsi:type="guigen:ResetButton" name="reset" label="Cancella"/>
            <widgets xsi:type="guigen:Button" name="hideFileds" label="Nascondi campi">
              <eventHandlers>
                <action xsi:type="guigen:ONOFFAction" targetWidgets="//@structure/@appWindow/@appArea/@contentPanels.1/@panels/@widgets.2 //@structure/@appWindow/@appArea/@contentPanels.1/@panels/@widgets.3 //@structure/@appWindow/@appArea/@contentPanels.1/@panels/@widgets.4 //@structure/@appWindow/@appArea/@contentPanels.1/@panels/@widgets.5"/>
              </eventHandlers>
            </widgets>
            <widgets xsi:type="guigen:Button">
              <eventHandlers>
                <action xsi:type="guigen:SequenceAction">
                  <actions xsi:type="guigen:ExecAction">
                    <results resultCode="ok">
                      <action xsi:type="guigen:JumpAction" jumpTo="//@structure/@appWindow/@appArea/@contentPanels.0"/>
                    </results>
                    <results resultCode="ko">
                      <action xsi:type="guigen:JumpAction" jumpTo="//@structure/@appWindow/@appArea/@contentPanels.2"/>
                    </results>
                    <results resultCode="stay">
                      <action xsi:type="guigen:JumpAction" jumpTo="//@structure/@appWindow/@appArea/@contentPanels.1"/>
                    </results>
                  </actions>
                </action>
              </eventHandlers>
            </widgets>
          </panels>
        </contentPanels>
        <contentPanels name="esempioButtons">
          <panels xsi:type="guigen:FormPanel" name="jumpingJackFlash" label="">
            <widgets xsi:type="guigen:Button" name="buttonJump" label="Vai al Form">
              <eventHandlers>
                <action xsi:type="guigen:JumpAction" jumpTo="//@structure/@appWindow/@appArea/@contentPanels.1"/>
              </eventHandlers>
            </widgets>
          </panels>
        </contentPanels>
      </appArea>
    </appWindow>
  </structure>
</guigen:GUIModel>
