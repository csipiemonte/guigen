import guigen;

String getRegistryID(ContentPanel cp) :
	'${'+cp.name+"}";

String jsLiteralValue(String s) :
	s==null? "null": "'"+s+"'";

String jsLiteralValue(int i) :
	i==null? "null": i;
	
String jsLiteralValue(boolean b) :
	b==null? "null": (b ? "true" : "false");
	
String jsLiteralValue(Void v) :
	"null";