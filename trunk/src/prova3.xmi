<?xml version="1.0" encoding="ASCII"?>
<guimda:GUIModel xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:guimda="http://www.csi.it/modeling" xsi:schemaLocation="http://www.csi.it/modeling metamodel/guimda.ecore">
  <structure>
    <mainFrame>
      <menubar>
        <topLevelMenu name="mnu_gestione" label="Gestione">
          <item name="mnu_gst_pagamenti" label="Gestione pagamenti"/>
        </topLevelMenu>
      </menubar>
      <titlebar/>
      <statusbar/>
      <contentPanels name="Start">
        <panels xsi:type="guimda:FormPanel">
          <layout xsi:type="guimda:NSEWCPanelLayout"/>
          <widgets xsi:type="guimda:Image">
            <layoutSpec xsi:type="guimda:NSEWCWidgetLayoutSpec" value="CENTER"/>
          </widgets>
        </panels>
      </contentPanels>
      <contentPanels name="gestPagamenti">
        <panels xsi:type="guimda:FormPanel" name="gestPagamenti">
          <layout xsi:type="guimda:NSEWCPanelLayout"/>
          <subpanels xsi:type="guimda:FormPanel" name="pPeriodo">
            <layout xsi:type="guimda:HorizontalFlowPanelLayout"/>
            <layoutSpec xsi:type="guimda:NSEWCWidgetLayoutSpec"/>
            <widgets xsi:type="guimda:RadioButtons">
              <radio name="anno" label="anno"/>
              <radio name="mese" label="mese"/>
              <radio name="altro" label="altro"/>
            </widgets>
            <widgets xsi:type="guimda:TextField" name="periodoDa" label="da:" dataType="DATA"/>
            <widgets xsi:type="guimda:TextField" name="periodoA" label="a:" dataType="DATA"/>
            <widgets xsi:type="guimda:ResetButton" name="annulla" label="annulla"/>
            <widgets xsi:type="guimda:ConfirmButton" name="conferma" label="conferma"/>
          </subpanels>
          <subpanels xsi:type="guimda:TabSetPanel" name="pDettagli">
            <layoutSpec xsi:type="guimda:NSEWCWidgetLayoutSpec" value="SOUTH"/>
            <tabs xsi:type="guimda:FormPanel" name="pIndCarica" label="Indennit&#xe0; di carica"/>
            <tabs xsi:type="guimda:FormPanel" name="pIndPresenza" label="Indennit&#xe0; di presenza"/>
          </subpanels>
        </panels>
      </contentPanels>
    </mainFrame>
  </structure>
  <beahviors>
    <flow startState="//@beahviors/@flow/@states.0">
      <states contentPanel="//@structure/@mainFrame/@contentPanels.0" name="START"/>
      <states contentPanel="//@structure/@mainFrame/@contentPanels.1" name="gstPagamenti">
        <possibleTransitions nextState="//@beahviors/@flow/@states.1" commandWidget="//@structure/@mainFrame/@contentPanels.1/@panels/@subpanels.0/@widgets.4"/>
      </states>
      <globalTransition jumpTo="//@beahviors/@flow/@states.1" menuCommand="//@structure/@mainFrame/@menubar/@topLevelMenu.0/@item.0"/>
    </flow>
  </beahviors>
</guimda:GUIModel>
